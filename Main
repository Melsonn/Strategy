package com.company;

public class Main {

    public static void main(String[] args) {
        PaymentStrategy strategy;

        String paymentMethod = "Kiwipayment";
        if(paymentMethod.equals("creditcard")){
            strategy = new CreditCardPayment();
        } else{
            strategy = new KiwiPayment();
        }

        strategy.pay(100);
    }
}
